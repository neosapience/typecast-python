{
    "openapi": "3.1.0",
    "info": {
        "title": "Typecast API",
        "version": "0.1.1",
        "x-logo": {
            "url": "https://typecast.ai/_ipx/_/image/logo/tc_logo.webp"
        }
    },
    "paths": {
        "/v1/text-to-speech": {
            "post": {
                "tags": [
                    "Text-to-Speech"
                ],
                "summary": "Text To Speech",
                "operationId": "text_to_speech_v1_text_to_speech_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/TTSRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ]
            }
        },
        "/v1/voices": {
            "get": {
                "tags": [
                    "Voices"
                ],
                "summary": "Get Voices",
                "operationId": "get_voices_v1_voices_get",
                "parameters": [
                    {
                        "name": "model",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "$ref": "#/components/schemas/TTSModel"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Voice"
                                    },
                                    "title": "Response Get Voices V1 Voices Get"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/voices/{voice_id}": {
            "get": {
                "tags": [
                    "Voices"
                ],
                "summary": "Get Voice",
                "operationId": "get_voice_v1_voices__voice_id__get",
                "parameters": [
                    {
                        "name": "voice_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Voice Id"
                        }
                    },
                    {
                        "name": "model",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "$ref": "#/components/schemas/TTSModel"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Voice"
                                    },
                                    "title": "Response Get Voice V1 Voices  Voice Id  Get"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/health": {
            "get": {
                "summary": "Health",
                "operationId": "health_health_get",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "EmotionEnum": {
                "type": "string",
                "enum": [
                    "normal",
                    "sad",
                    "happy",
                    "angry",
                    "regret",
                    "urgent",
                    "whisper",
                    "scream",
                    "shout",
                    "trustful",
                    "soft",
                    "cold",
                    "sarcasm",
                    "inspire",
                    "cute",
                    "cheer",
                    "casual",
                    "tunelv1",
                    "tunelv2",
                    "tonemid",
                    "toneup",
                    "tonedown"
                ],
                "title": "EmotionEnum"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "Output": {
                "properties": {
                    "volume": {
                        "type": "integer",
                        "maximum": 200.0,
                        "minimum": 0.0,
                        "title": "Volume",
                        "default": 100
                    },
                    "audio_pitch": {
                        "type": "integer",
                        "maximum": 12.0,
                        "minimum": -12.0,
                        "title": "Audio Pitch",
                        "default": 0
                    },
                    "audio_tempo": {
                        "type": "number",
                        "maximum": 2.0,
                        "minimum": 0.5,
                        "title": "Audio Tempo",
                        "default": 1.0
                    },
                    "audio_format": {
                        "type": "string",
                        "enum": [
                            "wav",
                            "mp3"
                        ],
                        "title": "Audio Format",
                        "description": "Audio format",
                        "default": "wav",
                        "example": "wav"
                    }
                },
                "type": "object",
                "title": "Output"
            },
            "Prompt": {
                "properties": {
                    "emotion_preset": {
                        "type": "string",
                        "enum": [
                            "happy",
                            "sad",
                            "normal",
                            "angry"
                        ],
                        "title": "Emotion Preset",
                        "description": "Emotion preset",
                        "default": "normal",
                        "example": "normal"
                    },
                    "emotion_intensity": {
                        "type": "number",
                        "maximum": 2.0,
                        "minimum": 0.0,
                        "title": "Emotion Intensity",
                        "default": 1.0
                    },
                    "speed": {
                        "type": "number",
                        "maximum": 2.0,
                        "minimum": 0.5,
                        "title": "Speed",
                        "default": 1.0
                    },
                    "intonation": {
                        "type": "integer",
                        "maximum": 2.0,
                        "minimum": -2.0,
                        "title": "Intonation",
                        "default": 0
                    }
                },
                "type": "object",
                "title": "Prompt"
            },
            "TTSModel": {
                "type": "string",
                "enum": [
                    "ssfm-v10",
                    "ssfm-v12",
                    "ssfm-v20",
                    "ssfm-v21"
                ],
                "title": "TTSModel"
            },
            "TTSRequest": {
                "properties": {
                    "voice_id": {
                        "type": "string",
                        "title": "Voice Id",
                        "description": "Voice ID",
                        "example": "67b5c03d9cf5c7788b5a4a32"
                    },
                    "text": {
                        "type": "string",
                        "title": "Text",
                        "description": "Text",
                        "example": "Hello. How are you?"
                    },
                    "model": {
                        "$ref": "#/components/schemas/TTSModel",
                        "description": "Voice model name",
                        "example": "ssfm-v20"
                    },
                    "language": {
                        "type": "string",
                        "title": "Language",
                        "description": "Language",
                        "examples": [
                            "eng"
                        ]
                    },
                    "prompt": {
                        "$ref": "#/components/schemas/Prompt"
                    },
                    "output": {
                        "$ref": "#/components/schemas/Output"
                    },
                    "seed": {
                        "type": "integer",
                        "title": "Seed"
                    }
                },
                "type": "object",
                "required": [
                    "voice_id",
                    "text",
                    "model"
                ],
                "title": "TTSRequest"
            },
            "ValidationError": {
                "properties": {
                    "loc": {
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        },
                        "type": "array",
                        "title": "Location"
                    },
                    "msg": {
                        "type": "string",
                        "title": "Message"
                    },
                    "type": {
                        "type": "string",
                        "title": "Error Type"
                    }
                },
                "type": "object",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "title": "ValidationError"
            },
            "Voice": {
                "properties": {
                    "voice_id": {
                        "type": "string",
                        "title": "Voice Id",
                        "description": "MongoDB ObjectId"
                    },
                    "voice_name": {
                        "type": "string",
                        "title": "Voice Name",
                        "description": "Voice name"
                    },
                    "model": {
                        "$ref": "#/components/schemas/TTSModel",
                        "description": "Voice model name"
                    },
                    "emotions": {
                        "items": {
                            "$ref": "#/components/schemas/EmotionEnum"
                        },
                        "type": "array",
                        "title": "Emotions",
                        "description": "Voice emotion"
                    }
                },
                "type": "object",
                "required": [
                    "voice_id",
                    "voice_name",
                    "model",
                    "emotions"
                ],
                "title": "Voice"
            }
        },
        "securitySchemes": {
            "ApiKeyAuth": {
                "type": "apiKey",
                "in": "header",
                "name": "X-API-KEY"
            }
        }
    }
}